{
  "manifest_version": 2,

  "name": "Send Text",
  "description": "This allows users to pass text from one workstation to another",
  "version": "1.0",

  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "identity",
    "identity.email"
  ],
  "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"
}
